   ],
   "source": [
    "for num in range (360,400):\n",
    "    sum = 0\n",
    "    temp = num\n",
    "    while temp>0:\n",
    "        digit = temp % 10\n",
    "        sum += digit ** 3\n",
    "        temp //= 10\n",
    "    if sum == num:\n",
    "        print(num,\"is a Armstrong number\")\n",
    "        break;\n",
    "    else:\n",
    "        continue\n"
   ]
